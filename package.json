{
  "name": "react-boilerplate",
  "description": "Webpack based react boilerplate.",
  "version": "1.0.0",
  "author": "Jan-Kanty Pawelski <contact@pawelski.io> (http://pawelski.io)",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/JanKantyPawelski/react-boilerplate.git"
  },
  "config": {
    "port": 9000
  },
  "scripts": {
    "clean": "rimraf build/ *.log",
    "build": "npm run clean && webpack --config webpack/config.build.js",
    "dev": "npm run clean && webpack-dev-server --config webpack/config.dev.js",
    "test": "jest",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint src/scripts/**/*",
    "lint:sass": "sass-lint -v src/styles/**/*"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.0",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.0",
    "babel-jest": "^11.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.0",
    "html-webpack-plugin": "^2.15.0",
    "jest-cli": "^11.0.0",
    "json-loader": "^0.5.0",
    "node-sass": "^3.4.0",
    "postcss-loader": "^0.8.0",
    "rimraf": "^2.5.0",
    "sass-lint": "^1.5.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.10.0"
  },
  "dependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}